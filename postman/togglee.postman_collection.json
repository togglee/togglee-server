										"value": "application/json"
									},
									{
										"key": "isTest",
										"type": "text",
										"value": "true"
									}
								],
								"body": {
									"mode": "graphql",
									"graphql": {
										"query": "mutation($email: String!, $password: String!){\n  createUser(email: $email, password: $password){ \n      success\n  }\n}",
										"variables": "{\n    \"email\": \"{{user_email}}\",\n    \"password\": \"{{user_password}}\"\n}"
									},
